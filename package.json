{
  "name": "douh",
  "version": "0.1.0",
  "description": "http based node.js web server framework",
  "author": "changchanghwnag",
  "homepage": "https://github.com/douhjs/douh",
  "scripts": {
    "test": "jest",
    "lint": "eslint .",
    "build": "rm -rf ./dist && npm run build:esm && npm run build:cjs",
    "build:cjs": "tsc --project tsconfig.build.json",
    "build:esm": "tsc --module esnext --outDir dist/esm --project tsconfig.build.json"
  },
  "main": "./dist/cjs/index.js",
  "types": "./dist/esm/index.d.ts",
  "esm": "./dist/esm/index.js",
  "license": "MIT",
  "dependencies": {
    "on-finished": "^2.4.1",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@types/on-finished": "^2.3.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.45.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.6.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  },
  "bugs": {
    "url": "https://github.com/douhjs/douh/issues"
  },
  "keywords": [
    "node.js",
    "node",
    "http",
    "framework",
    "web",
    "server",
    "application",
    "container",
    "router"
  ],
  "files": [
    "dist",
    "src/**/*.ts",
    "LICENSE",
    "README.md"
  ]
}
